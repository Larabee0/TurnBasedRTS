            </ui:VisualElement>
            <ui:VisualElement name="SpecialContainer" style="padding-left: 3px; padding-right: 3px; padding-top: 3px; padding-bottom: 3px; align-items: stretch; margin-left: 2px;">
                <ui:VisualElement style="flex-direction: row;">
                    <ui:Toggle name="SpecialToggle" style="flex-direction: row-reverse; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 0; flex-shrink: 1; max-width: 125px;" />
