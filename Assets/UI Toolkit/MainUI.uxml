                    <ui:Toggle name="FarmToggle" style="flex-direction: row-reverse; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 0; flex-shrink: 1; max-width: 125px;" />
                    <ui:Label text="Farm Level" display-tooltip-when-elided="true" enable-rich-text="true" style="padding-left: 3px;" />
                </ui:VisualElement>
                <ui:SliderInt picking-mode="Ignore" value="0" high-value="3" name="FarmSlider" style="margin-left: 2px; margin-right: 0; margin-top: 0; margin-bottom: 0;" />
