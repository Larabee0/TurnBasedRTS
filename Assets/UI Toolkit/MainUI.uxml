            </ui:VisualElement>
            <ui:VisualElement name="WalledContainer" style="background-color: rgb(54, 57, 63); padding-left: 3px; padding-right: 3px; padding-top: 3px; padding-bottom: 3px; border-top-left-radius: 3px; border-bottom-left-radius: 3px; border-top-right-radius: 3px; border-bottom-right-radius: 3px; margin-top: 3px;">
