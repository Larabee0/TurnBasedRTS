                    <ui:Button text="Load" display-tooltip-when-elided="true" name="LoadButton" style="padding-right: 3px; padding-left: 3px; width: 60px; padding-bottom: 5px; padding-top: 5px;" />
                </ui:VisualElement>
            </ui:VisualElement>
