                    <ui:Toggle name="ElevationToggle" style="flex-direction: row-reverse; margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0; padding-left: 0; flex-shrink: 1; max-width: 125px;" />
                    <ui:Label text="Elevation Level" display-tooltip-when-elided="true" style="padding-left: 3px;" />
                </ui:VisualElement>
                <ui:SliderInt picking-mode="Ignore" value="0" high-value="6" name="ElevationSlider" style="margin-left: 2px; margin-right: 0; margin-top: 0; margin-bottom: 0;" />
            </ui:VisualElement>
            <ui:VisualElement name="WaterContainer" style="padding-left: 3px; padding-right: 3px; padding-top: 3px; padding-bottom: 3px; align-items: stretch; margin-left: 2px;">
