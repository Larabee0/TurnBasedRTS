<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI Toolkit/MainStyleSheet.uss?fileID=7433441132597879392&amp;guid=cc220462b4caec241a4492003c5b2b91&amp;type=3#MainStyleSheet" />
    <ui:VisualElement picking-mode="Ignore" style="flex-direction: row; justify-content: space-between;">
        <ui:VisualElement name="Left" style="align-items: stretch; justify-content: flex-start; background-color: rgb(47, 49, 54); border-top-left-radius: 3px; border-bottom-left-radius: 3px; border-top-right-radius: 3px; border-bottom-right-radius: 3px; padding-left: 3px; padding-right: 3px; padding-top: 3px; padding-bottom: 3px;">
            <ui:VisualElement name="TerrainContainer" style="background-color: rgb(54, 57, 63); padding-left: 3px; padding-right: 3px; padding-top: 3px; padding-bottom: 3px; border-top-left-radius: 3px; border-bottom-left-radius: 3px; border-top-right-radius: 3px; border-bottom-right-radius: 3px;">
                <ui:RadioButtonGroup label="Terrain" value="5" choices="Sand,Grass,Mud,Stone,Snow,None" name="TerrainGroup" style="flex-direction: column;" />
            </ui:VisualElement>
            <ui:VisualElement name="ElevationContainer" style="padding-left: 3px; padding-right: 3px; padding-top: 3px; padding-bottom: 3px; align-items: stretch; margin-left: 2px;">
